{
  "name": "serverless-template",
  "description": "",
  "version": "0.1.0",
  "scripts": {
    "unit-test": "jest",
    "test-coverage": "jest --coverage",
    "lint": "eslint --fix .",
    "package": "node_modules/serverless/bin/serverless package",
    "config": "sls config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_ACCESS_KEY",
    "deploy": "sls deploy --stage $AWS_STAGE --region $AWS_REGION",
    "serve": "sls offline --stage local",
    "generate": "openapi-generator-cli generate -i spec/swagger.yaml -o src/models -g typescript-node"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "aws-sdk": "^2.851.0",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "fs-extra": "^9.1.0",
    "jest": "^27.2.5",
    "nyc": "^15.1.0",
    "openapi-typescript": "^3.0.3",
    "openapi-typescript-codegen": "^0.8.1",
    "serverless": "^2.28.7",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-epsagon": "^1.12.2",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-stage-manager": "^1.0.5",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "yaml": "^1.10.0",
    "yaml-edit": "^0.1.3"
  },
  "dependencies": {
    "@middy/core": "^3.1.0",
    "awilix": "^4.3.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "ts-custom-error": "^3.2.0",
    "uuid": "^3.3.2"
  },
  "nyc": {
    "branches": "80",
    "lines": "80",
    "functions": "80",
    "statements": "80"
  }
}
